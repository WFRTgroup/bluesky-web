mongo: mongod --dbpath ./docker-data/mongodb/ --port 27018 --logpath ./docker-logs/mongodb/mongodb.log
web: ./bin/bsp-web -d -b bluesky -o ./docker-data/output/ --path-prefix=bluesky --log-level=DEBUG --log-file=./docker-logs/web/bluesky-web.log
output_web: bash -c 'pkill -f "python -m http.server 8886"; true;' && cd ./docker-data/output/ && python -m http.server 8886
worker_all_met: celery -Q all-met -c 2 -A blueskyworker.tasks worker -l INFO -f ./docker-logs/worker/all-met/bluesky-web-worker-all-met.log
worker_dri: celery -Q dri -c 2 -A blueskyworker.tasks worker -l INFO -f ./docker-logs/worker/dri/bluesky-web-worker-dri.log
worker_nam: celery -Q nam -c 2 -A blueskyworker.tasks worker -l INFO -f ./docker-logs/worker/nam/bluesky-web-worker-nam.log
